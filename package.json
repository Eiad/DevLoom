{
  "name": "devloom",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "electron-start": "electron .",
    "react-start": "react-scripts start",
    "electron-dev": "concurrently \"BROWSER=none npm run react-start\" \"wait-on https://devloom.vercel.app && electron .\"",
    "electron-pack": "electron-builder"
  },
  "build": {
    "appId": "com.devloom.app",
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "dmg"
      ]
    }
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "next": "13.5.4",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.0.3",
    "prismjs": "^1.29.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "electron": "^27.0.0",
    "electron-builder": "^24.6.4",
    "eslint": "^8",
    "eslint-config-next": "13.5.4",
    "wait-on": "^6.0.1"
  }
}
