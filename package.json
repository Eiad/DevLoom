{
  "name": "devloom",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "electron-start": "electron .",
    "react-start": "react-scripts start",
    "electron-dev": "concurrently \"BROWSER=none npm run react-start\" \"wait-on https://devloom.net && electron .\"",
    "electron-pack": "electron-builder"
  },
  "build": {
    "appId": "com.devloom.app",
    "files": [
      "out/**/*",
      "index.js",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "dmg"
      ]
    }
  },
  "dependencies": {
    "@vercel/analytics": "^1.1.1",
    "dompurify": "^3.0.6",
    "electron-serve": "^1.1.0",
    "js-yaml": "^4.1.0",
    "lorem-ipsum": "^2.0.8",
    "next": "13.5.4",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.0.3",
    "prismjs": "^1.29.0",
    "react": "^18",
    "react-dom": "^18",
    "react-markdown": "^9.0.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "electron": "^27.0.0",
    "electron-builder": "^24.6.4",
    "eslint": "^8",
    "eslint-config-next": "13.5.4",
    "wait-on": "^6.0.1"
  }
}
